@{
}

@model List<SongViewModel>

@*View all songs snippet data*@
@foreach (var Song in Model)
{
    <div>
        <h2>@Song.Title</h2>
        <h4>@Song.Vocalist</h4>
        <h4>
            Tabulatura:
        </h4>
        <img src=@Url.Content(Song.Tab) alt="Song Tab">
        
    </div>
}
